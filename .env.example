# Brood environment configuration (copy to .env)

# OpenAI
OPENAI_API_KEY=
OPENAI_API_KEY_BACKUP=
OPENAI_IMAGE_STREAM=0
OPENAI_IMAGE_USE_RESPONSES=0

# OpenRouter
OPENROUTER_API_KEY=
OPENROUTER_API_BASE=
OPENROUTER_HTTP_REFERER=
OPENROUTER_X_TITLE=

# Realtime routing (intent canvas + always-on vision)
# Values: openai_realtime | gemini_flash
# gemini_flash transport resolution:
#   1) OPENROUTER_API_KEY (OpenRouter responses, chat fallback)
#   2) GEMINI_API_KEY / GOOGLE_API_KEY (Gemini generateContent)
BROOD_REALTIME_PROVIDER=
BROOD_CANVAS_CONTEXT_REALTIME_PROVIDER=
BROOD_INTENT_REALTIME_PROVIDER=
BROOD_MOTHER_INTENT_REALTIME_PROVIDER=
# Optional model overrides (defaults: OpenAI -> gpt-realtime-mini, Gemini -> gemini-3-flash-preview)
BROOD_CANVAS_CONTEXT_REALTIME_MODEL=
BROOD_INTENT_REALTIME_MODEL=
BROOD_MOTHER_INTENT_REALTIME_MODEL=

# Anthropic (receipt analyzer / text)
ANTHROPIC_API_KEY=

# Gemini
GEMINI_API_KEY=
GOOGLE_API_KEY=

# Imagen
IMAGEN_API_KEY=
IMAGEN_VERTEX_API_KEY=
GOOGLE_APPLICATION_CREDENTIALS=
GOOGLE_CLOUD_PROJECT=
GOOGLE_CLOUD_LOCATION=
GOOGLE_PROJECT_ID=

# Flux / BFL
# If BFL_API_KEY/FLUX_API_KEY are absent and OPENROUTER_API_KEY is set,
# Brood uses OpenRouter image generation for flux provider requests.
BFL_API_KEY=
FLUX_API_KEY=

# Brood memory toggle
BROOD_MEMORY=0
